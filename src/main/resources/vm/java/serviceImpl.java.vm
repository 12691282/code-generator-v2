package ${packageName}.service.impl;

import com.baomidou.mybatisplus.core.conditions.query.QueryWrapper;
import com.baomidou.mybatisplus.extension.service.impl.ServiceImpl;
import org.springframework.stereotype.Service;
import com.github.pagehelper.PageInfo;
import io.renren.common.page.TableSupport;
import io.renren.common.utils.DatabaseUtils;
import ${packageName}.${daoPackageName}.${daoClassName};
import ${packageName}.service.${serviceClassName};
import ${packageName}.${modelPackageName}.${prototypeClassName};

import java.util.List;

/**
 * ${functionName}Service业务层处理
 * 
 * @author ${author}
 * @date ${datetime}
 */
@Service
public class ${serviceClassName}Impl extends ServiceImpl<${daoClassName}, ${prototypeClassName}>
                                     implements ${serviceClassName}
{
    /**
     * 查询${functionName}列表
     * 
     * @param ${className} ${functionName}
     * @return ${functionName}
     */
    @Override
    public PageInfo select${prototypeClassName}List(${prototypeClassName} ${className})
    {
        TableSupport.startPage();
        QueryWrapper query = new QueryWrapper();
        DatabaseUtils.notNullFieldCopy(${className}, query);
        List<${prototypeClassName}> list =  this.baseMapper.selectList(query);
        return TableSupport.getPageInfo(list);
    }


}
